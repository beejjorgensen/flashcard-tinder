{
  "name": "flashcard-tinder-server",
  "version": "1.0.0",
  "description": "Backend server for flashcard-tinder app",
  "contributors": [
    "Steven O'Campo",
    "Dan Winslow",
    "Latoyya Smith",
    "John Wells",
    "Wesley Harvey"
  ],
  "main": "server/index.js",
  "keywords": [
    "flashcards"
  ],
  "engines": {
    "node": "8.9.x",
    "npm": "5.6.x",
    "yarn": "1.3.x"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "cd client && yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && { yarn && yarn build; }"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MagicalLambda/flashcard-tinder"
  },
  "author": "Magical Lambda Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MagicalLambda/flashcard-tinder"
  },
  "homepage": "https://github.com/MagicalLambda/flashcard-tinder#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "mongoose": "^4.13.9",
    "nodemon": "^1.14.11",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
